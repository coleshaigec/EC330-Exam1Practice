// Implement as state machine
void bubbleSort(DoublyLinkedList* A){
  // Check for degenerate list and return before algorithm run if detected
  if (A->getSize() <= 1) {
    return;
  }

  // Initialize two variables: one to fence off sorted max region and one to track current element
  Node* curr = A->head;
  Node* max = A->findTail();

  while (curr != max) {
    
  }  
}
