#include <iostream>
#include <vector>
#include <cmath>

using namespace std;

// Swaps two elements at specified indices in an array
void swap(vector<int>& A, int a, int b) {
  int temp = A[b];
  A[b] = A[a];
  A[a] = temp;
}

